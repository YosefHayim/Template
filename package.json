{
  "name": "readme-profile-tech-stack-sync",
  "version": "1.0.0",
  "description": "Automatically sync tech stack from all public GitHub repositories to profile README",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "analyze": "node scripts/analyze-repos.js",
    "aggregate": "node scripts/aggregate-tech-stack.js",
    "update-readme": "node scripts/update-readme.js",
    "sync": "npm run analyze && npm run aggregate && npm run update-readme",
    "test": "node scripts/test.js"
  },
  "keywords": [
    "github",
    "readme",
    "tech-stack",
    "automation",
    "github-actions"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@specfy/stack-analyser": "^1.27.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0"
  }
}
